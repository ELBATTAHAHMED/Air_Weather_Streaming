            "uid": "timescale"
          },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH grouped AS (\n  SELECT\n    CONCAT(city, ' / ', parameter) AS metric,\n    COUNT(*)::double precision AS value\n  FROM air_quality_metrics\n  WHERE $__timeFilter(window_start)\n    AND window_start >= ($__timeTo()::timestamptz - INTERVAL '10 minute')\n    AND window_start <= $__timeTo()::timestamptz\n    AND status = 'DANGER'\n    AND status ~* '${status:regex}'\n    AND city ~* '${city:regex}'\n    AND parameter ~* '${parameter:regex}'\n    AND window_duration ~* '${window_duration:regex}'\n  GROUP BY metric\n  ORDER BY value DESC, metric\n  LIMIT 1\n)\nSELECT $__timeTo()::timestamptz AS \"time\", metric, value\nFROM grouped\nUNION ALL\nSELECT $__timeTo()::timestamptz AS \"time\", 'No active DANGER' AS metric, 0::double precision AS value\nWHERE NOT EXISTS (SELECT 1 FROM grouped)",
          "refId": "A"
        }
